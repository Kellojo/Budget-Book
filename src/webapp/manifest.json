{

    "sap.ui5": {

        "models": {
            "device": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "Database": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "AppInfo": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "AppHeader": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "User": {
                "type": "sap.ui.model.json.JSONModel"
            }
        },
        
        "rootView": {
            "viewName": "com.budgetBook.view.app",
            "type": "XML",
            "async": true
        },

        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewPath": "com.budgetBook.view",
                "viewType": "XML",
                "async": true,
                "transition": "slide",
                "controlId": "idApp",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": ""
                }
            },

            "routes": [
                {
                    "pattern": "",
                    "name": "overview",
                    "target": "overview"
                },
                {
                    "pattern": "welcome",
                    "name": "welcome",
                    "target": "welcome"
                },
                {
                    "pattern": "transaction/{transactionId}",
                    "name": "transaction",
                    "target": "transaction"
                },
                {
                    "pattern": "auth",
                    "name": "auth",
                    "target": "auth"
                }
            ],

            "targets": {
                "overview": {
                    "viewName": "overview"
                },
                "welcome": {
                    "viewName": "welcome"
                },
                "transaction": {
                    "viewName": "transaction"
                },
                "auth": {
                    "viewName": "auth"
                }
            }
        }
    }
}