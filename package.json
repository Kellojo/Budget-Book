{
  "name": "budget-book",
  "version": "1.1.2",
  "description": "A simple little app designed to help tracking your income and expenses.",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "start-web": "http-server ./",
    "start-auto-less-builder": "node ./src/bin/less-builder.js",
    "build-styles": "./src/bin/build-less.sh",
    "build-preloads": "grunt",
    "remove-preloads": "rm -rf './src/webapp/Component-preload.js' && rm -rf './src/webapp/lib/kellojo.m/src/library-preload.js'",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "npm run build-preloads && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kellojo/Budget-Book.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kellojo/Budget-Book/issues"
  },
  "homepage": "https://github.com/Kellojo/Budget-Book#readme",
  "build": {
    "appId": "com.budgetP",
    "productName": "BudgetP",
    "mac": {
      "category": "public.app-category.finance",
      "publish": [
        "github"
      ]
    }
  },
  "devDependencies": {
    "chokidar": "^3.4.0",
    "electron": "^8.5.2",
    "electron-builder": "^22.4.1",
    "grunt": "^1.3.0",
    "grunt-openui5": "^0.16.0",
    "http-server": "^0.12.3"
  },
  "dependencies": {
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.0",
    "electron-window-state": "^5.0.3",
    "firebase": "^7.14.6",
    "jsstore": "^3.7.6",
    "menubar": "^9.0.1"
  }
}
